type Query {
    experiments(user: String): [Experiment]
}

type Mutation {
    createExperiment(
        user: String,
        name: String,
        description: String,
        startDate: String,
        endDate: String
    ): Experiment
}

type Experiment {
    id: String,
    user: String,
    name: String,
    description: String,
    startDate: String,
    endDate: String
}